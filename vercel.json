{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "installCommand": "npm ci",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_GOOGLE_CLIENT_ID": "@vite_google_client_id"
  }
}

